# Database
DATABASE_URL="postgresql://username:password@localhost:5432/prague_family_events?schema=public"

# Server Configuration
NODE_ENV="development"
PORT=3001

# Frontend URL (for CORS)
FRONTEND_URL="http://localhost:3000"

# OpenWeatherMap API (optional - for weather-based scoring)
# Get free API key at: https://openweathermap.org/api
OPENWEATHER_API_KEY=""

# Cron Jobs
ENABLE_CRON="false"  # Set to "true" in production to enable daily scraping

# Rate Limiting
SCRAPER_DELAY_MS=2000  # Delay between scraper requests in milliseconds
